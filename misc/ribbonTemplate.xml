<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui">
    <ribbon startFromScratch="false">
        <tabs>
            <tab id="obtTab" label="OutbreakTools">
                <group id="customGroupTabMan" getLabel="getLLLang">
                    <button id="btnShowHideVar" getLabel="getLLLang" image="hide" size="large" onAction="clickRibbonShowHideVar"/>
                    <button id="btnAddRows" getLabel="getLLLang" imageMso="QueryShowTable" size="large" onAction="clickRibbonAddRows"/>
                    <button id="btnResize" getLabel="getLLLang" imageMso="SheetRowsDelete" size="large" onAction="clickRibbonResize"/>
                </group>
                <group id="customGroupTabFilt" getLabel="getLLLang">
                    <button id="btnRemFilt" getLabel="getLLLang" imageMso="FilterClearAllFilters" size="large" onAction="clickRibbonRemoveFilter"/>
                    <button id="btnSortTab" getLabel="getLLLang" image="sort" size="large" onAction="clickRibbonSortTable"/>
                </group>
                <group id="customGroupTabGeo" getLabel="getLLLang">
                    <button id="btnGeo" getLabel="getLLLang" image="globe" size="large" onAction="clickRibbonGeo"/>
                </group>
                <group id = "customGroupAdmin" getLabel = "getLLLang">
                    <button id="btnExport"
                    getLabel = "getLLLang" image="download" size="large" onAction="clickRibbonExport"/>
                </group>
                <group id="customGroupAna" getLabel="getLLLang">
                    <button id="btnCalc" imageMso="CalculateSheet" size="large"
            getLabel = "getLLLang" onAction="clickRibbonCalculate"/>
                    <button id="btnExpAna" image="anaexport" size="large"
                    getLabel = "getLLLang" onAction="clickRibbonExportAnalysis"/>
                    <button id="btnOpenLab" image="openvarlabels" size="large"
                getLabel = "getLLLang" onAction="clickRibbonOpenVarLab"/>
                </group>
                <group id="customGroupTabPrint" getLabel="getLLLang">
                    <button id="btnOpenPrint" getLabel="getLLLang" image="openprint" size="normal" onAction="clickRibbonOpenPrint"/>
                    <button id="btnClosePrint" getLabel="getLLLang" imageMso="PrintPreviewClose" size="normal" onAction="clickRibbonClosePrint"/>
                    <button id="btnRotateHead" getLabel="getLLLang" imageMso="ObjectRotateFree" size="normal" onAction="clickRibbonRotateAll"/>
                    <button id="btnRowHeight" getLabel="getLLLang" imageMso="RowHeight" size="normal" onAction="clickRibbonRowHeight"/>
                    <button id="btnPrintLL" getLabel="getLLLang" imageMso="FilePrint" size="normal" onAction="clickRibbonPrintLL"/>
                </group>
                <group id="customGroupAdvanced">
                    <button id="btnAdvanced" image="settings" getLabel="getLLLang" size="large" onAction="clickRibbonAdvanced"/>
                </group>
            </tab>
        </tabs>
    </ribbon>
</customUI>